Begin Queue
QUEUE_NAME   = cloud
PRIORITY     = 30
INTERACTIVE  = NO
#RCVJOBS_FROM = cluster
FAIRSHARE    = USER_SHARES[[default,1]]
DESCRIPTION  = Send jobs to remove cluster(s)
RC_HOSTS = all
RES_REQ = select[azurecchost && !azureccmpi && !azurecclowprio]
#RC_DEMAND_POLICY = 0
End Queue

Begin Queue
